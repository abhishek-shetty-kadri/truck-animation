{
  "navigationFallback": {
    "rewrite": "/truck-animation/index.html",
    "exclude": ["/truck-animation/assets/*"]
  },
  "routes": [
    {
      "route": "/truck-animation/assets/*",
      "rewrite": "/assets/{path}"     
    },
    {
      "route": "/truck-animation",
      "rewrite": "/index.html"     
    }
  ],
  "mimeTypes": {
    ".json": "text/json"
  }
}
